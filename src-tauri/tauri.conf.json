{
  "productName": "AEZAKMI",
  "version": "2.0.0",
  "identifier": "com.aezakmi.antidetect",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "width": 1280,
        "height": 820,
        "resizable": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "publisher": "AEZAKMI Team",
    "copyright": "Copyright Â© 2025 AEZAKMI Team. All rights reserved.",
    "homepage": "https://github.com/CJ-aezakmi/aezakmi",
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "binaries/launch_playwright.exe"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  
  "plugins": {
    "shell": {
      "open": true
    }
  }
  
}