{
  "package": {
    "productName": "AEZAKMI",
    "version": "2.0.0"
  },
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "tauri": {
    "bundle": {
      "identifier": "com.aezakmi.antidetect",
      "active": true,
      "targets": "all",
      "resources": [
        "binaries/launch_playwright.exe"
      ]
    },
    "windows": [
      {
        "label": "main",
        "width": 1280,
        "height": 820,
        "resizable": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    },
    "plugins": {
      "shell": {
        "open": true
      }
    }
  }
}